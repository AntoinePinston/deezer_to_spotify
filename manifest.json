{
    "manifest_version": 3,
    "name": "deezerToSpotify",
    "description": "Convert deezer playlist to spotify",
    "version": "1.0",
    "action": {
        "default_popup": "main.html",
        "default_icon": "hello_extensions.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["scripts/storage.js", "scripts/request.js"]
          
        }
    ]
    
}
